{
  "name": "medusa-plugin-order-monitoring",
  "private": true,
  "type": "module",
  "workspaces": [
    "dev/medusa",
    "order-monitoring"
  ],
  "scripts": {
    "start:medusa": "nodemon --watch \"order-monitoring\" --ignore \"order-monitoring/src\" --ignore \"order-monitoring/thunder\" --spawn --exec \"npm start -w medusa\" --delay 2 --verbose",
    "start": "concurrently -n \"build:order-monitoring\" \"npm run build -w medusa-plugin-order-monitoring && npm run build -w medusa\"  && concurrently -n \"start:medusa,watch:order-monitoring\" \"npm run start:medusa\" \"npm run watch -w medusa-plugin-order-monitoring\"  --timings --prefix \"[{name}] [{time}] =>\""
  },
  "dependencies": {
    "@agilorunner/gitdeploy": "^2.4.0",
    "@agilorunner/run": "^2.4.0",
    "concurrently": "^8.2.2",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.1",
    "openapi-filter": "^3.2.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/fs-extra": "^11.0.4"
  }
}
